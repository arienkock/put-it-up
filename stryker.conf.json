{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "mutate": [
    "scripts/**/*.js"
  ],
  "testRunner": "karma",
  "karma": {
    "projectType": "custom",
    "configFile": "karma.conf.js",
    "config": {
      "browsers": ["ChromeHeadless"]
    }
  },
  "timeoutFactor": 4,
  "reporters": [
    "html",
    "clear-text",
    "progress"
  ],
  "coverageAnalysis": "off"
}